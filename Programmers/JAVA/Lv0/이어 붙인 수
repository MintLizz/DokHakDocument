class Solution {
    public int solution(int[] num_list) {
        int answer = 0;
        String result1 = "";
        String result2 = "";
        
        for(int i=0; i < num_list.length; i++){
            if(num_list[i] % 2 == 0){
                result1 += Integer.toString(num_list[i]);
            }else{
                result2 += Integer.toString(num_list[i]);
            }
        }
        answer = Integer.parseInt(result1) + Integer.parseInt(result2);
        return answer;
    }
}
