import java.util.*;
class Solution {
    public int[] solution(int[] num_list) {
        
        List<Integer> result = new ArrayList<>();
        
        Arrays.sort(num_list);
        
        
        for(int i=0; i < 5; i++){
            result.add(num_list[i]);
        }
        
        int[] answer = new int[result.size()];
        
        for(int i=0; i< result.size(); i++){
            answer[i] = result.get(i);
        }
        
        return answer;
    }
}
